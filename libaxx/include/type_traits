#ifndef LIBAXX_TYPE_TRAITS
#define LIBAXX_TYPE_TRAITS

#include "external/libcxx/include/__config"
#include "external/libcxx/include/type_traits"

namespace std {

template <class T>
struct remove_reference {
  typedef T type;
};
template <class T>
struct remove_reference<T&> {
  typedef T type;
};
template <class T>
struct remove_reference<T&&> {
  typedef T type;
};

}  // namespace std

#endif
